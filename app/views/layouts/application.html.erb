<!DOCTYPE html>
<html>
<head>
  <title>StackTest</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if flash[:alert] %>
  <div class="flash-error">
    <span><%=flash[:alert]%></span>
  </div>
<%end%>

<%= yield %>

</body>
</html>
